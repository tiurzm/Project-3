{
  "name": "GroupAway_Travel_App",
  "version": "1.0.0",
  "description": "An app that a group of friends or family members can use to help plan a trip they will be taking together",
  "author": "Stephanie Truong, Tiur Mawaddah, Cera Smart, and Jennifer Gaumnitz",
  "license": "MIT",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiurzm/Project-3"
  },
  "homepage": "https://groupaway-3.herokuapp.com/",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "npm run kill && concurrently \"nodemon\" \"npm run client\"",
    "server": "npm run kill && nodemon",
    "client": "cd client && npm start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "kill": "kill-port 3001",
    "lint": "eslint --quiet .",
    "lint:fix": "eslint --fix .",
    "tests": "npm run kill && NODE_ENV=test nyc --reporter=html --reporter=text mocha tests/**/*.spec.js --exit",
    "test:coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nodemonConfig": {
    "events": {
      "restart": "npm run kill",
      "crash": "npm run kill"
    },
    "delay": "3000",
    "ignore": [
      "client/*"
    ]
  },
  "nyc": {
    "exclude": [
      "client"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^5.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "kill-port": "^1.6.0",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/moment-timezone": "^4.4.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@pusher/chatkit-client": "^1.13.4",
    "@pusher/chatkit-server": "^2.4.0",
    "axios": "^0.19.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.10.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash-messages": "^0.1.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "if-env": "^1.0.4",
    "local-storage": "^2.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mongodb": "^3.5.3",
    "mongoose": "^5.8.11",
    "node-sass": "^7.0.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "passport-session": "^1.0.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-google-maps": "^9.4.5",
    "react-places-autocomplete": "^7.2.1"
  }
}
